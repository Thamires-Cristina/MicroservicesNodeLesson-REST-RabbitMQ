datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Order {
  id        String   @id @default(cuid())
  userId    String
  items     String   // JSON.stringify dos itens
  total     Float
  status    String
  createdAt DateTime @default(now())
  cancelledAt DateTime?
}
